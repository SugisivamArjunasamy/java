import java.util.Scanner;
class Sum 
{
  void sum(int a,int b)
  {
    int c=a+b;
    System.out.println("Sum is:"+c);
  }
  void sum(double a,double b)
  {
    double c=(double)a-b;
    System.out.println("subtraction is:"+c);
  }
  void sum(int a)
  {
    int b=(a*a);
    System.out.println("Square is:"+b);
  }
  void sum(int a,double b)
  {
    double c=(double)a/b;
    System.out.println("Quotient is:"+c);
  }
}
 
class Main
{   
  
  public static void main(String[] args) 
  {
    Scanner sc=new Scanner(System.in);
    int a,b,c,d;
    double e,f,g;
    System.out.print("Enter two elements for addition :");
    a=sc.nextInt();
    b=sc.nextInt();
    System.out.print("Enter two elements for subtraction :");
    e=sc.nextDouble();
    f=sc.nextDouble();
    System.out.println("Enter one element for square :");
    c=sc.nextInt();
    System.out.println("Enter two element for division :");
    g=sc.nextDouble();
    d=sc.nextInt();
    

    Sum s=new Sum();
    s.sum(a,b);
    s.sum(e,f);
    s.sum(c);
    s.sum(d,g);
    
  }
    
}
