import java.util.Scanner;

public class exthree {
	public static void main(String[]org) {
		int num1,num2,num3;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the num1 :");
		num1=sc.nextInt();
		System.out.println("Enter the num2 :");
		num2=sc.nextInt();
		System.out.println("Enter the num3 :");
		num3=sc.nextInt();
		
		if(num1>num2) {
			if(num1>num3) {
				System.out.println("The number 1 is bigger");
			}
			else {
				System.out.println("The number 3 is bigger");
			}
		}
		else if(num2>num1) {
			if(num2>num3) {
				System.out.println("The number 2 is bigger");
			}
			else {
				System.out.println("The number 3 is bigger");
			}
		}
		else {
			System.out.println("The number 3 is bigger");
		}
		
		
	}
}
